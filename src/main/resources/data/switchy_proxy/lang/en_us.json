{
  "lib39:enable_enhanced_lang": true,
  "commands": {
    "switchy_proxy": {
      "add": {
        "command": "/switchy proxy add {} {}",
        "help": "adds a proxy pattern",
        "success": "Successfully added {} proxy to {}",
        "fail": {
          "preset": "That preset doesn't exist! Try {}",
          "module": "The proxies module is disabled! Try {}",
          "invalid": "Invalid proxy format. Should include {}"
        }
      },
      "remove": {
        "command": "/switchy proxy remove {} {}",
        "help": "removes a proxy",
        "success": "Successfully removed {} proxy from {}",
        "fail": {
          "preset": "That preset doesn't exist! Try {}",
          "module": "The proxies module is disabled! Try {}",
          "pattern": "That proxy doesn't exist!"
        }
      },
      "placeholder": {
        "preset": "[name]",
        "proxy": "[pattern]"
      }
    }
  }
}
