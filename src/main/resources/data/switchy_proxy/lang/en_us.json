{
  "lib39:enable_enhanced_lang": true,
  "commands": {
    "switchy_proxy": {
      "add": {
        "command": "/switchy proxy add {} {}",
        "help": "adds a proxy pattern",
        "success": "Successfully added {} proxy to {}",
        "fail": {
          "preset": "That preset doesn't exist! Try {}",
          "module": "The proxies module is disabled! Try {}",
          "invalid": "Invalid proxy format. Should include {}"
        }
      },
      "remove": {
        "command": "/switchy proxy remove {} {}",
        "help": "removes a proxy",
        "success": "Successfully removed {} proxy from {}",
        "fail": {
          "preset": "That preset doesn't exist! Try {}",
          "module": "The proxies module is disabled! Try {}",
          "pattern": "That proxy doesn't exist!"
        }
      },
      "placeholder": {
        "preset": "[name]",
        "proxy": "[pattern]"
      }
    }
  },
  "switchy": {
    "switchy_proxy": {
      "module": {
        "proxies": {
          "description": "A module that holds \"proxy tags\" that allow using other presets' nicknames for individual messages.",
          "enabled": "You will be able to use and add proxies with /switchy proxy.",
          "disabled": "You will not be able to use or add proxies.",
          "warning": "All proxy tags will be cleared!"
        }
      }
    }
  }
}
